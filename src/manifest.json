{
  "manifest_version": 3,

  "name": "Simple CleanUp",
  "description": "A simple Chrome extension to quickly clean your browsing data.",
  "version": "2.0.0",

  "action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Simple CleanUp"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  },

  "permissions": ["browsingData", "cookies", "downloads", "history", "storage"]
}
